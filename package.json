{
  "name": "CAS_First_Project",
  "version": "1.0.0",
  "author": "Nephi Szvoboda",
  "dependencies": {
    "finalhandler": "^0.3.6",
    "handlebars": "^4.0.8",
    "jquery": "^3.2.1",
    "moment": "^2.18.1",
    "rollup": "^0.42.0",
    "rollup-watch": "^4.0.0",
    "serve-static": "^1.9.2"
  },
  "devDependencies": {
    "nodemon": "^1.11.0"
  },
  "scripts": {
    "compileJs": "rollup --input app/reminder-overview/reminder-overview.controller.js --format iife --output dist/bundle.js",
    "build-hb": "handlebars -m app/templates/> dist/templates.js",
    "watch-hb": "nodemon -e handlebars,js --ignore dist/ -x \"npm run compileJs | npm run build-hb\""
  }
}